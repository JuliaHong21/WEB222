//Task 1
function capNotFirstLetter(str){ 
  return str.charAt(0).toLowerCase() + str.substring(1,str.length).toUpperCase(); 
}


//Task 2
var data = people.map(p=>p);
var swichCase = data.map(p => capNotFirstLetter(p.first_name));
console.log(swichCase); 


